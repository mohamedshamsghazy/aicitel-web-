{
    "kind": "collectionType",
    "collectionName": "faqs",
    "info": {
        "singularName": "faq",
        "pluralName": "faqs",
        "displayName": "FAQ",
        "description": "Frequently Asked Questions"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {
        "i18n": {
            "localized": true
        }
    },
    "attributes": {
        "question": {
            "type": "string",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "answer": {
            "type": "blocks",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "category": {
            "type": "enumeration",
            "enum": [
                "Career",
                "Partner"
            ],
            "required": true
        },
        "order": {
            "type": "integer"
        }
    }
}